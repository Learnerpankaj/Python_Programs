# Remove the Duplicates from the list

list1 = [1,3,2,1,3,2,4,5,6,3,5,7,1]
list2 =[]

    # Approach 1
for x in list1:
    if x not in list2:
        list2.append(x)
print(list2)

    #Approach 2
for i in range(0,len(list1)-1):
    if list1[i] not in list2:
        list2.append(list1[i])
print(list2)
